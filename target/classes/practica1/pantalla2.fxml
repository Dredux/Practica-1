<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<VBox alignment="CENTER" prefHeight="197.0" prefWidth="286.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="practica1.Pantalla2Controller">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <TableView fx:id="tablaBocadillos" prefHeight="200.0" prefWidth="200.0">
        <columns>
            <TableColumn fx:id="nombreTableColumn" prefWidth="75.0" text="Nombre" />
            <TableColumn fx:id="precioTableColumn" prefWidth="75.0" text="Precio" />
        </columns>
    </TableView>

    <TextField fx:id="nombreTextField" promptText="Introduce un nombre..." />
    <TextField fx:id="precioTextField" promptText="Introduce el precio..." />

    <Button mnemonicParsing="false" onAction="#guardarBocadillos" prefHeight="26.0" prefWidth="108.0" text="Guardar">
        <font>
            <Font name="Calibri" size="12.0" />
        </font>
    </Button>
    <Button fx:id="pantallaHello" onAction="#irAPantallaHello" text="Ir a pantalla Hello" />
</VBox>